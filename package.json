{
  "name": "react-tools-cli",
  "version": "0.1.3",
  "description": "react command line utility",
  "main": "index.js",
  "bin": {
    "react": "./bin/react.js"
  },
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hengkx/react-tools-cli.git"
  },
  "keywords": [
    "react",
    "saga"
  ],
  "author": "Tom Xu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hengkx/react-tools-cli/issues"
  },
  "homepage": "https://github.com/hengkx/react-tools-cli#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "cross-spawn": "^5.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-runtime"
    ]
  }
}